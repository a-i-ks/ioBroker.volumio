        "main": "build/main.js",
        "icon": "volumio.png",
        "enabled": true,
        "extIcon": "https://raw.githubusercontent.com/a-i-ks/ioBroker.volumio/master/admin/volumio.png",
        "readme": "https://github.com/a-i-ks/ioBroker.volumio/blob/master/README.md",
        "loglevel": "info",
        "tier": 3,
        "mode": "daemon",
        "type": "multimedia",
        "compact": true,
        "connectionType": "local",
        "dataSource": "poll",
        "adminUI": {
            "config": "json"
